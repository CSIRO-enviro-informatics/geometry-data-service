    "/search/latlng/{x},{y}": {
      "get": {
        "tags": [
          "search"
        ],
        "summary": "Get individual geometries that contain the specified position",
        "description": "",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "operationId": "searchGeomByPosition",
        "parameters": [
          {
            "name": "x",
            "in": "path",
            "description": "x",
            "required": true,
            "schema": {
              "type": "number",
              "example": 142.111
            }
          },
          {
            "name": "y",
            "in": "path",
            "description": "y",
            "required": true,
            "schema": {
              "type": "number",
              "example": -32.1231
            }
          },
          {
            "name": "crs",
            "in": "query",
            "description": "crs",
            "required": false,
            "schema": {
              "type": "number"
            }
          }
        ]
      }
    },
    "/search/wkt": {
      "post": {
        "tags": [
          "search"
        ],
        "summary": "Get individual geometries that contain the specified position",
        "description": "",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SearchResult"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "operationId": "searchGeomByWkt",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "dataset",
                  "wkt"
                ],
                "properties": {
                  "dataset": {
                    "type": "string",
                    "example": "asgs16_sa3"
                  },
                  "wkt": {
                    "type": "string",
                    "example": "POLYGON((150.90464969227213 -33.60051712906992,151.45396609852213 -33.60051712906992,151.45396609852213 -34.07048711355048,150.90464969227213 -34.07048711355048,150.90464969227213 -33.60051712906992))"
                  },
                  "operation": {
                    "type": "string",
                    "enum": [
                      "intersects",
                      "contains",
                      "overlaps"
                    ],
                    "example": "intersects"
                  },
                  "crs": {
                    "type": "number",
                    "example": 4326
                  }
                }
              }
            }
          }
        }
      }
    },
    "/search/latlng/{x},{y}/dataset/{dataset_id}": {
      "get": {
        "tags": [
          "search"
        ],
        "summary": "Get individual geometries that contain the specified position and are associated with the dataset specified.",
        "description": "",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "operationId": "searchGeomByPositionAndDataset",
        "parameters": [
          {
            "name": "x",
            "in": "path",
            "description": "x",
            "required": true,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "y",
            "in": "path",
            "description": "y",
            "required": true,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "dataset_id",
            "in": "path",
            "description": "dataset id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "crs",
            "in": "query",
            "description": "y",
            "required": false,
            "schema": {
              "type": "number"
            }
          }
        ]
      }
    }
